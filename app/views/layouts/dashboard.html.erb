<!DOCTYPE html>
<html>
  <head>
    <title>ECommerce01</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag "application_dashboard", media: "all",
      :"data-turbolinks-track" => "reload" %>
    <%= javascript_include_tag "application_dashboard",
      :"data-turbolinks-track" => "reload" %>
      <%= javascript_include_tag "https://www.gstatic.com/charts/loader.js" %>
    <% if content_for?(:head) %>
      <%= yield(:head) %>
    <% end %>
  </head>

  <body>
    <%= render "layouts/admin/admin_header" %>
    <div class="content">
      <% flash.each do |message_type, message| %>
        <div class="alert alert-<%= message_type %> mt-50 mess_dasboard">
          <%= message %>
        </div>
      <% end %>
      <%= yield %>
    </div>
  </body>
</html>
